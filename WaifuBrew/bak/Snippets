// in configpane

    try {
        SynthLookAndFeel laf = new SynthLookAndFeel();
        System.out.println(RESOURCE_PATH + "lof.xml");

        laf.load(this.getClass().getResourceAsStream(RESOURCE_PATH + "lof.xml"), this.getClass());
        System.out.println("test");
        UIManager.setLookAndFeel(laf);
    } catch (Exception e) {
        e.printStackTrace();
    }


    add(new JSlider() {{
        setOrientation(JSlider.HORIZONTAL);
        setMinimum(0);
        setMaximum(100);
        setVisible(true);
    }});